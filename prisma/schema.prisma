// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./database.db" 
}

generator client {
  provider = "prisma-client-js"
}

model GuildConfig {
  id Int @id @default(autoincrement())
  guildId String
  roleToPingId String
  channelToSendMsgId String
}
